{
  "name": "@graphql-inspector/action",
  "version": "3.4.2",
  "description": "GraphQL Inspector functionality for GitHub Actions",
  "repository": {
    "type": "git",
    "url": "kamilkisiela/graphql-inspector",
    "directory": "packages/action"
  },
  "author": {
    "name": "Kamil Kisiela",
    "email": "kamil.kisiela@gmail.com",
    "url": "https://github.com/kamilkisiela"
  },
  "license": "MIT",
  "bin": {
    "graphql-inspector-github": "./dist/index.js"
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./*": {
      "require": "./dist/*.js",
      "import": "./dist/*.mjs"
    }
  },
  "typings": "dist/index.d.ts",
  "keywords": [
    "graphql",
    "graphql-inspector",
    "tools",
    "probot",
    "probot-plugin",
    "github"
  ],
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf dist/",
    "deploy": "now --docker",
    "prebuild": "yarn clean",
    "prepack": "bob prepack",
    "prepublishOnly": "yarn build",
    "test": "jest"
  },
  "dependencies": {
    "@actions/core": "1.10.0",
    "@actions/github": "4.0.0",
    "@graphql-inspector/commands": "3.4.0",
    "@graphql-inspector/core": "3.5.0",
    "@graphql-inspector/github": "3.4.2",
    "js-yaml": "4.1.0",
    "probot": "11.4.1",
    "tslib": "^2.0.0"
  },
  "devDependencies": {
    "@types/js-yaml": "4.0.5"
  },
  "publishConfig": {
    "directory": "dist",
    "access": "public"
  },
  "sideEffects": false,
  "typescript": {
    "definition": "dist/index.d.ts"
  }
}
