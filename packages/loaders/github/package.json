{
  "name": "@graphql-inspector/github-loader",
  "version": "3.1.3",
  "description": "Load GraphQL Schema and Documents from GitHub repository",
  "keywords": [
    "graphql",
    "graphql-inspector",
    "graphql-inspector-plugin",
    "tools"
  ],
  "sideEffects": false,
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./*": {
      "require": "./dist/*.js",
      "import": "./dist/*.mjs"
    }
  },
  "typings": "dist/index.d.ts",
  "typescript": {
    "definition": "dist/index.d.ts"
  },
  "author": {
    "name": "Kamil Kisiela",
    "email": "kamil.kisiela@gmail.com",
    "url": "https://github.com/kamilkisiela"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "kamilkisiela/graphql-inspector",
    "directory": "packages/plugins/loaders/github"
  },
  "peerDependencies": {
    "graphql": "^0.13.0 || ^14.0.0 || ^15.0.0 || ^16.0.0"
  },
  "dependencies": {
    "@graphql-tools/github-loader": "^7.1.1",
    "tslib": "^2.0.0"
  },
  "scripts": {
    "prepack": "bob prepack"
  }
}
