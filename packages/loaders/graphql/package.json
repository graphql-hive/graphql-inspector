{
  "name": "@graphql-inspector/graphql-loader",
  "version": "3.4.2",
  "description": "Load GraphQL Schema and Documents from GraphQL files (.graphql, .gql etc)",
  "repository": {
    "type": "git",
    "url": "kamilkisiela/graphql-inspector",
    "directory": "packages/plugins/loaders/graphql"
  },
  "author": {
    "name": "Kamil Kisiela",
    "email": "kamil.kisiela@gmail.com",
    "url": "https://github.com/kamilkisiela"
  },
  "license": "MIT",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "exports": {
    ".": {
      "require": {
        "types": "./dist/typings/index.d.cts",
        "default": "./dist/cjs/index.js"
      },
      "import": {
        "types": "./dist/typings/index.d.ts",
        "default": "./dist/esm/index.js"
      },
      "default": {
        "types": "./dist/typings/index.d.ts",
        "default": "./dist/esm/index.js"
      }
    },
    "./package.json": "./package.json"
  },
  "typings": "dist/typings/index.d.ts",
  "keywords": [
    "graphql",
    "graphql-inspector",
    "graphql-inspector-plugin",
    "tools"
  ],
  "scripts": {
    "prepack": "bob prepack"
  },
  "peerDependencies": {
    "graphql": "^14.0.0 || ^15.0.0 || ^16.0.0"
  },
  "dependencies": {
    "@graphql-tools/graphql-file-loader": "^7.3.2",
    "tslib": "^2.0.0"
  },
  "devDependencies": {
    "graphql": "16.6.0"
  },
  "publishConfig": {
    "directory": "dist",
    "access": "public"
  },
  "sideEffects": false,
  "typescript": {
    "definition": "dist/typings/index.d.ts"
  },
  "type": "module"
}