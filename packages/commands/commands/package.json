{
  "name": "@graphql-inspector/commands",
  "version": "3.4.4",
  "description": "Plugin system for commands in GraphQL Inspector",
  "repository": {
    "type": "git",
    "url": "kamilkisiela/graphql-inspector",
    "directory": "packages/commands/commands"
  },
  "author": {
    "name": "Kamil Kisiela",
    "email": "kamil.kisiela@gmail.com",
    "url": "https://github.com/kamilkisiela"
  },
  "license": "MIT",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "exports": {
    ".": {
      "require": {
        "types": "./dist/typings/index.d.cts",
        "default": "./dist/cjs/index.js"
      },
      "import": {
        "types": "./dist/typings/index.d.ts",
        "default": "./dist/esm/index.js"
      },
      "default": {
        "types": "./dist/typings/index.d.ts",
        "default": "./dist/esm/index.js"
      }
    },
    "./package.json": "./package.json"
  },
  "typings": "dist/typings/index.d.ts",
  "keywords": [
    "graphql",
    "graphql-inspector",
    "graphql-inspector-command",
    "tools"
  ],
  "scripts": {
    "prepack": "bob prepack"
  },
  "peerDependencies": {
    "@graphql-inspector/config": "3.4.3",
    "@graphql-inspector/loaders": "3.4.4",
    "graphql": "^14.0.0 || ^15.0.0 || ^16.0.0",
    "yargs": "17.2.1"
  },
  "dependencies": {
    "tslib": "^2.0.0"
  },
  "publishConfig": {
    "directory": "dist",
    "access": "public"
  },
  "sideEffects": false,
  "typescript": {
    "definition": "dist/typings/index.d.ts"
  },
  "type": "module"
}